{
  "version": 2,
  "buildCommand": "echo 'Build complete'",
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/styles.css", "destination": "/public/styles.css" },
    { "source": "/app.js", "destination": "/public/app.js" },
    { "source": "/dp.webp", "destination": "/public/dp.webp" }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10,
      "memory": 2048
    }
  }
}
